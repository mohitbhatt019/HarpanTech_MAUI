<?xml version="1.0" encoding="utf-8" ?><views:ContentPageBase xmlns="http://schemas.microsoft.com/dotnet/2021/maui"             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"             x:Class="HarpenTech.Views.RecievePage.RecieveView"             xmlns:skia="clr-namespace:SkiaSharp.Extended.UI.Controls;assembly=SkiaSharp.Extended.UI"             xmlns:views="clr-namespace:HarpenTech.Views"             xmlns:vm="clr-namespace:HarpenTech.ViewModels"             xmlns:i="clr-namespace:System.Windows.Input;assembly=System.Windows"             Shell.FlyoutBehavior="Flyout"             xmlns:searchHandler="clr-namespace:HarpenTech.Services.Handler"             xmlns:model="clr-namespace:HarpenTech.Models.Container"             xmlns:material="http://schemas.enisn-projects.io/dotnet/maui/uraniumui/material"             xmlns:uranium="http://schemas.enisn-projects.io/dotnet/maui/uraniumui"              x:DataType="vm:RecieveViewModel"     Title="          Search Vehicle"             >    <!--<Shell.TitleView>        <Label TextColor="#FFFFFF" Text="Search Vehicle"  VerticalOptions="CenterAndExpand" FontSize="14"   HorizontalOptions="CenterAndExpand"/>    </Shell.TitleView>-->    <ContentPage.ToolbarItems>        <ToolbarItem Text="{OnPlatform Default='&lt;'}" Command="{Binding BackClickCommand}">            <ToolbarItem.IconImageSource>                <FontImageSource Glyph="&lt;" FontFamily="YourFontFamily" Size="200" Color="White"/>            </ToolbarItem.IconImageSource>        </ToolbarItem>    </ContentPage.ToolbarItems>    <VerticalStackLayout Margin="8" >        <Grid>            <Grid.RowDefinitions>                <RowDefinition Height="Auto"/>                <RowDefinition Height="Auto"/>            </Grid.RowDefinitions>            <Grid.ColumnDefinitions>                <ColumnDefinition />                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>                <ColumnDefinition/>            </Grid.ColumnDefinitions>            <!--AutoComplete Entry-->            <material:AutoCompleteTextField  TextColor="Black" BackgroundColor="Transparent" x:Name="SearchBarText" Grid.Row="0"                                             Grid.Column="1" BorderThickness="0"    Grid.ColumnSpan="13" Title="Search reg no" HeightRequest="45"                                             ItemsSource="{Binding ShipNameOnly} " >            </material:AutoCompleteTextField>            <!--Diveder AutoComplete-->            <BoxView HeightRequest="0.8" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="12" StyleClass="Divider" />             <!--Search Button-->            <ImageButton BackgroundColor="Transparent" Clicked="SearchClickButton"  Grid.Row="0" Source="search.png" VerticalOptions="EndAndExpand" Grid.Column="13" Grid.ColumnSpan="2" WidthRequest="40" HeightRequest="35"  />                        <!--Clear Button-->            <ImageButton BackgroundColor="Transparent" x:Name="Cleartextbutton"  Grid.Row="0" Clicked="ClearSearch" VerticalOptions="EndAndExpand" Source="close.png" Margin="0,0,-10,0" Grid.Column="15" Grid.ColumnSpan="2" WidthRequest="30"  HeightRequest="35"/>        </Grid>        <!-- INDICATOR -->        <skia:SKLottieView          Source="loadtest.json"          RepeatCount="10"          IsVisible="{Binding IsBusy}"          IsAnimationEnabled="{Binding IsBusy}"          WidthRequest="100"          HeightRequest="100"          VerticalOptions="Center"          HorizontalOptions="Center">        </skia:SKLottieView>    </VerticalStackLayout></views:ContentPageBase>