<?xml version="1.0" encoding="utf-8" ?><ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"             x:Class="HarpenTech.Views.RecievePage.SelectVehicleView"             xmlns:colorchange="clr-namespace:HarpenTech.ViewModels"             x:DataType="colorchange:InspectContainerViewModel"              xmlns:material="http://schemas.enisn-projects.io/dotnet/maui/uraniumui/material"             xmlns:uranium="http://schemas.enisn-projects.io/dotnet/maui/uraniumui"               Title="         Select vehicle"
              >    <!--<Shell.TitleView>        <Label TextColor="#FFFFFF" Text="Select Vehicle              "  VerticalOptions="CenterAndExpand" FontSize="14"   HorizontalOptions="CenterAndExpand"/>    </Shell.TitleView>-->    <Grid>        <StackLayout Padding="8"  >            <Grid >                <Grid.ColumnDefinitions>                    <ColumnDefinition   />                    <ColumnDefinition />                    <ColumnDefinition />                    <ColumnDefinition />                    <ColumnDefinition   />                    <ColumnDefinition />                    <ColumnDefinition />                    <ColumnDefinition />                    <ColumnDefinition />                    <ColumnDefinition />                    <ColumnDefinition />                    <ColumnDefinition />                    <ColumnDefinition />                    <ColumnDefinition />                </Grid.ColumnDefinitions>                <material:AutoCompleteTextField x:Name="SearchEntry"  TextColor="Black" BackgroundColor="Transparent" Grid.Row="1"                                 BorderThickness="0"   Grid.Column="1"  Grid.ColumnSpan="9" Title="Search reg no" HeightRequest="45"  >                </material:AutoCompleteTextField>                <!--Diveder AutoComplete-->                <BoxView HeightRequest="0.8" VerticalOptions="End" Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="9" StyleClass="Divider" />                <!--<Label x:Name="searchlabel" IsVisible="false" Grid.Row="0" Grid.ColumnSpan="3" Text="Search reg no." Margin="0,0,0,0" Grid.Column="1"></Label>                <Entry x:Name="SearchEntry" Focused="SearchEntryFocus" Unfocused="SearchEntryUnFocus" FontSize="14" Grid.Row="1"  Placeholder="Search reg no." VerticalOptions="EndAndExpand" Grid.Column="1"  Grid.ColumnSpan="9"  Margin="-9" />-->                <ImageButton Clicked="SearchClick" Grid.Row="1" Source="search.png" Grid.Column="10" Margin="0,0,-10,0" VerticalOptions="End"  HeightRequest="35"/>                <ImageButton x:Name="Cleartextbutton"  Grid.Row="1" Clicked="ClearSearch" VerticalOptions="End" Source="close.png" Grid.Column="12"                              HeightRequest="30" />            </Grid >            <Grid Padding="20" Margin="0,-15,0,0"  >                <ListView SelectionMode="None" x:Name="myListView" ItemsSource="{Binding Status}"  SeparatorVisibility="None" HasUnevenRows="True"   ItemSelected="OnItemSelected"     >                    <ListView.ItemTemplate>                        <DataTemplate x:DataType="{x:Type x:String}">                            <ViewCell Tapped="ViewCell_Tapped" >                                <Grid Padding="0" BackgroundColor="#D9D9D9"  Background="#D9D9D9" Margin="4" >                                    <Grid.RowDefinitions  >                                    </Grid.RowDefinitions>                                    <Grid.ColumnDefinitions>                                        <ColumnDefinition   />                                        <ColumnDefinition />                                        <ColumnDefinition />                                        <ColumnDefinition />                                        <ColumnDefinition />                                        <ColumnDefinition />                                        <ColumnDefinition />                                        <ColumnDefinition />                                    </Grid.ColumnDefinitions>                                    <!--Transporter Name-->                                    <Label  ZIndex="100" Grid.Row="0"  FontSize="13" Grid.Column="0"  Grid.ColumnSpan="4" Text="{Binding}" HorizontalOptions="Start" VerticalOptions="Center" />                                    <Label  Grid.Row="0" Grid.Column="4" Grid.ColumnSpan="3" FontSize="13" Text="Reg No " HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand"   />                                    <Label ZIndex="100" Grid.Row="0" ScaleY="1.5" ScaleX="1.5" Grid.Column="7" Padding="2" HorizontalOptions="Center" Grid.ColumnSpan="1" FontSize="20" Text=">" TextColor="#0000ff"   />                                    <Label Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="6" />                                </Grid>                            </ViewCell>                        </DataTemplate>                    </ListView.ItemTemplate>                </ListView>            </Grid>        </StackLayout>    </Grid></ContentPage>