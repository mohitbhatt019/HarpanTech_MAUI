<?xml version="1.0" encoding="utf-8" ?><ContentPage    x:Class="HarpenTech.Views.Dispatch.DispatchView"    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"    xmlns:vm="clr-namespace:HarpenTech.ViewModels"    x:DataType="vm:DispatchViewModel"    Shell.FlyoutBehavior="Flyout"     Title="                Dispatch">    <!--<Shell.TitleView>        <Label TextColor="#FFFFFF" Text="Dispatch       "  VerticalOptions="CenterAndExpand" FontSize="14"   HorizontalOptions="CenterAndExpand"/>    </Shell.TitleView>-->            <ContentPage.ToolbarItems >        <ToolbarItem Text="{OnPlatform Default='&lt;'}" Command="{Binding BackClickCommand}">            <ToolbarItem.IconImageSource>                <FontImageSource Glyph="&lt;" FontFamily="YourFontFamily" Size="200" Color="White"/>            </ToolbarItem.IconImageSource>        </ToolbarItem>    </ContentPage.ToolbarItems>
    <Grid HeightRequest="500" Margin="25,0,20,0" VerticalOptions="Start" >        <Grid.RowDefinitions>            <RowDefinition Height="50" />            <RowDefinition Height="50" />            <RowDefinition Height="35" />            <RowDefinition />            <RowDefinition />            <RowDefinition />            <RowDefinition />            <RowDefinition />            <RowDefinition />            <RowDefinition/>        </Grid.RowDefinitions>        <Grid.ColumnDefinitions>            <ColumnDefinition />            <ColumnDefinition />            <ColumnDefinition />            <ColumnDefinition />            <ColumnDefinition />            <ColumnDefinition />            <ColumnDefinition />            <ColumnDefinition />        </Grid.ColumnDefinitions>        <SearchBar x:Name="SearchBarHolder" Keyboard="Numeric" Placeholder="Search Reg no..." Grid.Column="0" Margin="-25,0,0,0"  VerticalOptions="Start" HorizontalOptions="Start" WidthRequest="380" />
        <Button  BackgroundColor="Transparent" HeightRequest="35" Padding="10" WidthRequest="30" />        <!--Transporter-->        <Label  Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="3" Text="Transporter" VerticalOptions="CenterAndExpand" />        <Entry Grid.ColumnSpan="5" Grid.Row="1" Grid.Column="3"  Placeholder="Transporter" VerticalOptions="CenterAndExpand"/>        <!--Bookingref-->        <Label Grid.ColumnSpan="3"  Grid.Row="2" Grid.Column="0"   Text="Booking Ref" VerticalOptions="CenterAndExpand" />        <Entry  Grid.Row="2" Grid.Column="3" Grid.ColumnSpan="5" Keyboard="Numeric" Placeholder="BookingRef" VerticalOptions="CenterAndExpand"/>         <!--ISO-->        <Button Grid.Row="3" Grid.Column="0" Command="{Binding DispatchEditClickCommand}"  ZIndex="100" BackgroundColor="Transparent"  HeightRequest="35"  WidthRequest="70"/>        <Entry  Grid.Row="3" Grid.Column="0" WidthRequest="60" Margin="30,0,0,0" Placeholder="ISO" TextColor="Black"  />        <!--ISO-->        <Button Grid.Row="3" Grid.Column="2" ZIndex="100" Command="{Binding DispatchEditClickCommand}" VerticalOptions="CenterAndExpand" BackgroundColor="Transparent"  HeightRequest="35"  WidthRequest="70"/>        <Entry  Grid.Row="3" Grid.Column="2"  Placeholder="ISO" TextColor="Black" WidthRequest="60" Margin="30,0,0,0"  />        <!--ISO-->        <Button Grid.Row="3" Grid.Column="4" ZIndex="100" Command="{Binding DispatchEditClickCommand}" BackgroundColor="Transparent"  HeightRequest="35"  WidthRequest="70"/>        <Entry  Grid.Row="3" Grid.Column="4"  Placeholder="ISO" TextColor="Black"  WidthRequest="60" Margin="30,0,0,0" />        <!--ISO-->        <Button Grid.Row="3" Grid.Column="6" ZIndex="100" Command="{Binding DispatchEditClickCommand}" BackgroundColor="Transparent"   HeightRequest="35"  WidthRequest="70"/>        <Entry  Grid.Row="3" Grid.Column="6" Placeholder="ISO"  TextColor="Black"  WidthRequest="60" Margin="30,0,0,0" />        <!--Transporter-->        <Label  Grid.Row="4" Grid.Column="0" Grid.ColumnSpan="3" Text="Transporter" VerticalOptions="CenterAndExpand"/>        <Entry Grid.ColumnSpan="5" Grid.Row="4" Grid.Column="3"  Placeholder="Transporter" VerticalOptions="CenterAndExpand"/>        <!--Commodity-->        <Label  Grid.Row="5" Grid.Column="0" Grid.ColumnSpan="3" Text="Commodity" VerticalOptions="CenterAndExpand"/>        <Entry Grid.ColumnSpan="5" Grid.Row="5" Grid.Column="3"  Placeholder="Commodity" VerticalOptions="CenterAndExpand"/>    </Grid></ContentPage>